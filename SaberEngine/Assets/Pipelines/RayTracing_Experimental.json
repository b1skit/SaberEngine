		{
			"GraphicsSystem": "Tonemapping",
			"Inputs": [
				{
					"GraphicsSystem": "RayTracing_Experimental",
					"TextureDependencies": [
						{
							"SourceName": "RayTracingTarget",
							"DestinationName": "TonemappingTarget"
